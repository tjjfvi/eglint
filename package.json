{
  "name": "eglint",
  "version": "1.0.0",
  "description": "",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "test": "npx tsc && node dist/tests/runner",
    "test-watch": "onchange -ik 'src/**/*' 'tests/**/*' -e 'tests/crossproduct/*/out/**/*' -- npm run test-watch_ --silent",
    "test-watch_": "printf '\\e[2J\\e[3J\\e[H'; npm run test",
    "tcu": "rm -rf tests/crossproduct/*/out; npm run test -- -u",
    "debug": "node --inspect-brk -r ts-node/register tests/runner",
    "index": "create-index -X ts -x ts -x tsx -Sui src/ -c {}; npm run lint"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/diff": "^5.0.0",
    "@types/eslint": "^7.2.9",
    "@types/node": "^14.14.37",
    "@types/yargs": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "chalk": "^4.1.1",
    "create-index-better": "^1.5.0",
    "diff": "^4.0.2",
    "eslint": "^7.24.0",
    "node-watch": "^0.7.1",
    "onchange": "^7.1.0",
    "piscina": "^3.1.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.3.2",
    "yargs": "^6.6.0"
  }
}
