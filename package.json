{
  "name": "eglint",
  "version": "1.0.0",
  "description": "",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "test": "npx tsc && node dist/tests/runner",
    "test-watch": "onchange -ik 'src/**/*' 'tests/**/*' -e 'tests/crossproduct/*/out/**/*' -- npm run test-watch_ --silent",
    "test-watch_": "printf '\\e[2J\\e[3J\\e[H'; npm run test",
    "tcu": "rm -rf tests/crossproduct/*/out; npm run test -- -u",
    "debug": "node --inspect-brk -r ts-node/register tests/runner",
    "index": "create-index -X ts -x ts -x tsx -Sui src/ -c {}; npm run lint",
    "postinstall": "rm node_modules/eglint; ln -s ../.. node_modules/eglint",
    "3up": "rollup -cw",
    "build-3up": "NODE_ENV=\"production\" rollup -c"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/diff": "^5.0.0",
    "@types/eslint": "^7.2.9",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@types/yargs": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "chalk": "^4.1.1",
    "create-index-better": "^1.5.0",
    "diff": "^4.0.2",
    "eslint": "^7.24.0",
    "node-watch": "^0.7.1",
    "onchange": "^7.1.0",
    "piscina": "^3.1.0",
    "rollup": "^2.53.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-styles": "^3.14.1",
    "rollup-plugin-terser": "^7.0.2",
    "stylus": "^0.54.8",
    "ts-node": "^9.1.1",
    "typescript": "^4.3.2",
    "yargs": "^6.6.0"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  }
}